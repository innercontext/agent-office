{
  "name": "agent-office",
  "version": "0.6.17",
  "description": "A multi-agent office management system CLI",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "agent-office": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:innercontext/agent-office.git"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "cli",
    "typescript",
    "commander",
    "multi-agent",
    "office",
    "management"
  ],
  "author": "Richard Anaya",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@toon-format/toon": "^2.1.0",
    "agent-office": "^0.6.16",
    "better-sqlite3": "^12.3.1",
    "commander": "^14.0.3",
    "croner": "^10.0.1",
    "postgres": "^3.4.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.15.14",
    "@vitest/coverage-v8": "^4.0.18",
    "prettier": "^3.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
